{
  "name": "countarr",
  "version": "1.0.0",
  "private": true,
  "description": "Media server statistics dashboard for Radarr, Sonarr, Bazarr, Prowlarr, Jellyseerr, and Emby/Jellyfin",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "db:generate": "turbo run db:generate --filter=@countarr/server",
    "db:migrate": "turbo run db:migrate --filter=@countarr/server",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20.0.0"
  }
}
